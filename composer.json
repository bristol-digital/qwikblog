{
  "name": "bristol-digital/qwikblog",
  "description": "A simple file-based blog system for Laravel with Tailwind CSS",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "blog", "markdown", "tailwind"],
  "authors": [
    {
      "name": "Bristol Digital",
      "email": "code@bristol.digital"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/view": "^10.0|^11.0",
    "illuminate/routing": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "BristolDigital\\QwikBlog\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BristolDigital\\QwikBlog\\QwikBlogServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
